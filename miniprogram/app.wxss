/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 



.nav_navigation{
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: #FFFFFF; 
}
.nav_title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-size:18px;
  letter-spacing: 0.5px;
  
}

.nav_back{
  width: 5px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
}

.nav_leftarrow{
   height: 45px;
   width: 50px;
   position: absolute;
   top: 0;
   left: 0;
   display:flex;
   justify-content:center; 
   align-items:center;
}
.nav_leftarrow image{
  width: 9px;
  height: 16px;
}
